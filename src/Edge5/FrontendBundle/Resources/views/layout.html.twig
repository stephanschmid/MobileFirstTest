<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=0">
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">

    {% block metadata %}
        <title>{{ 'page_title_default'|trans }}</title>
        <meta name="description" content="{{ 'page_description_default'|trans }}">

        <meta property="og:locale" content="de_DE"/>
        <meta property="og:site_name" content="{{ 'page_name'|trans }}"/>
        <meta property="og:title" content="{{ 'og_title_default'|trans }}"/>
        <meta property="og:description" content="{{ 'page_description_default'|trans }}"/>
        <meta property="og:url" content="http:{{ url('_home', {}, true) }}"/>
        <meta property="og:image" content="http:{{ url('liip_imagine_filter', {'filter':'opengraph', 'path' : 'bundles/edge5frontend/img/og.png'}, true) }}"/>
        <meta property="og:type" content="website"/>
    {% endblock %}

    {# <meta name="apple-itunes-app" content="app-id={{ 'ios_app_id_value'|trans }}"> #}

    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/edge5frontend/img/app-icons/32_favicon.png') }}"/>

    <link rel="apple-touch-icon-precomposed" sizes="152x152" href="{{ asset('bundles/edge5frontend/img/app-icons/152.png') }}"/>
    <link rel="apple-touch-icon-precomposed" sizes="120x120" href="{{ asset('bundles/edge5frontend/img/app-icons/120.png') }}"/>
    <link rel="apple-touch-icon-precomposed" sizes="76x76" href="{{ asset('bundles/edge5frontend/img/app-icons/76.png') }}"/>
    <link rel="apple-touch-icon-precomposed" href="{{ asset('bundles/edge5frontend/img/app-icons/57.png') }}"/>

    {% block stylesheets %}
        {% stylesheets filter='compass' output='css/compiled/frontend.css'
        '@Edge5FrontendBundle/Resources/public/scss/main.scss' %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet"/>
        {% endstylesheets %}
    {% endblock %}

    {# go build a custom modernizr!! #}
    <script src="{{ asset('bundles/edge5frontend/js/vendor/modernizr.custom.js') }}"></script>
</head>
<body>
<!--[if lt IE 7]>
<p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
<![endif]-->

{% block content %}
{% endblock %}

{# Require JS/jQuery 2.1.14 #}
{% block javascripts %}
<script data-main="{{ asset('bundles/edge5frontend/js/main.js') }}" src="{{ asset('bundles/edge5frontend/js/vendor/require.js') }}"></script>
{% endblock %}

